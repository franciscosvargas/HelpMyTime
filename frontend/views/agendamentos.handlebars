<div class="dashboard-header">
	<h1 class="dashboard-header-title animated slideInLeft">Horários de {{user.name}}</h1>
</div>


<div id="schedule-carousel">
	<div class="carousel-cell">
		<h4 class="schedule-cell-title">Segunda</h4>
		<div class="cell-divider"></div>
		{{#if schedules.monday}}
			{{#each schedules.monday}}
				
				<div id="{{this._id}}" onclick="openDialogViewSchedule2('{{this._id}}','{{this.time}}','{{this.client.name}}', '{{this.client.phone}}')" class="schedule-cell unavailable-cell">
					<p class="schedule-cell-time">{{this.time}}</p>
					<p class="schedule-cell-name">Agendado</p>
					<div class="schedule-cell-divider"></div>
					<p class="schedule-cell-client">{{this.client.name}}</p>
				</div>	
				
			{{/each}}
		{{else}}
			<div class="schedule-cell empty-cell">
				<p class="schedule-cell-name">Não há horários</p>
			</div>
		{{/if}}

	</div>
